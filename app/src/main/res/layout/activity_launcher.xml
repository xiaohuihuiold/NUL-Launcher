<?xml version="1.0" encoding="utf-8"?>
<com.xhh.launcher.custom.view.LauncherRootView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/launcher"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.xhh.launcher.custom.view.DragLayer
        android:id="@+id/launcher_draglayer"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clipChildren="false"
        android:clipToPadding="false">

        <com.xhh.launcher.custom.view.Workspace
            android:layout_width="match_parent"
            android:layout_height="match_parent">


            <com.xhh.launcher.custom.view.Hotseat
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </com.xhh.launcher.custom.view.Hotseat>

        </com.xhh.launcher.custom.view.Workspace>

    </com.xhh.launcher.custom.view.DragLayer>


</com.xhh.launcher.custom.view.LauncherRootView>
